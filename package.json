{
  "name": "markdown-table-editor",
  "displayName": "Markdown Table Editor",
  "description": "Edit Markdown tables in an Excel-like Webview",
  "version": "0.0.1",
  "publisher": "kojimang",
  "repository": {
    "type": "git",
    "url": "https://github.com/kojimang/markdown_table_editor.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],

  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdownTableEditor.editTable",
        "title": "Edit Markdown Table"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "markdownTableEditor.editTable",
          "when": "editorLangId == markdown",
          "group": "modification"
        }
      ]
    }
  },
  "keybindings": [
    {
      "command": "markdownTableEditor.editTable",
      "key": "ctrl+k e",
      "mac": "cmd+k e",
      "when": "editorTextFocus && editorLangId == markdown"
    }
  ],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "css-loader": "^6.8.1",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
